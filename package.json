{
  "name": "ngx-bootstrap",
  "version": "0.0.1",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "serve": "http-server . -p 5557",
    "rollup": "rollup -f iife -c -o dist/js/bundle.es2015.js",
    "es5": "tsc --target es5 --allowJs dist/js/bundle.es2015.js --out dist/js/bundle.js",
    "minify": "uglifyjs dist/js/bundle.js --screw-ie8 --compress --mangle --output dist/js/bundle.min.js",
    "build": "npm run clean && npm run rollup && npm run es5 && npm run minify",
    "build_test": "npm run rollup && npm run es5 && npm run minify"
  },
